{
  "name": "skima-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "bin": "src/index.js",
  "scripts": {
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "test": "vitest run --coverage",
    "pkg:build": "npx @yao-pkg/pkg . --compress GZip"
  },
  "pkg": {
    "targets": [
      "node20-win-x64",
      "node20-linux-x64",
      "node20-macos-x64",
      "node20-macos-arm64"
    ],
    "scripts": [
      "node_modules/.prisma/client/**/*.js",
      "node_modules/@prisma/client/**/*.js"
    ],
    "assets": [
      "node_modules/.prisma/client/query_engine-*",
      "node_modules/.prisma/client/*.node",
      "node_modules/@prisma/engines/*",
      "prisma/schema.prisma",
      "prisma/migrations/**/*"
    ],
    "outputPath": "../src-tauri/binaries"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.0",
    "@yao-pkg/pkg": "^6.0.0",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "vitest": "^2.0.0"
  }
}
